/* GNU ld script
 * Use the shared library, but some functions are only in
 * the static library, so try that secondarily. */
OUTPUT_FORMAT("elf32-i386", "elf32-i386",
	      "elf32-i386")
GROUP ( /opt/uclibc/usr/lib/uclibc_nonshared.a /opt/uclibc/lib/libc.so.1 AS_NEEDED ( /opt/uclibc/lib/ld-uClibc.so.1 ) )
GROUP ( AS_NEEDED ( /opt/uclibc/lib/libubacktrace.so.1 ) )
